		<root-zone>language-root.stylus</root-zone>
		<name>Stylus</name>
		<detectors>
			<extension>styl</extension>
		</detectors>
		<default-extension>styl</default-extension>
	</language>
	<meta>
		<name>Stylus.sugar</name>
		<description>Stylus sugar for MacRabbit's Espresso code editor</description>
		<author>Alex Ledak</author>
		<version>1.1</version>
		<download format="git">git://github.com/aljs/Stylus.sugar.git</download>
		<url>http://github.com/aljs/Stylus.sugar</url>
	</meta>
</settings>